{
  "expo": {
    "name": "cicatrify",
    "slug": "cicatrify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "fonts": [
      {
        "name": "Ubuntu-Bold",
        "file": "./assets/fonts/Ubuntu-Bold.ttf"
      },
      {
        "name": "Ubuntu-BoldItalic",
        "file": "./assets/fonts/Ubuntu-BoldItalic.ttf"
      },
      {
        "name": "Ubuntu-Italic",
        "file": "./assets/fonts/Ubuntu-Italic.ttf"
      },
      {
        "name": "Ubuntu-Light",
        "file": "./assets/fonts/Ubuntu-Light.ttf"
      },
      {
        "name": "Ubuntu-LightItalic",
        "file": "./assets/fonts/Ubuntu-LightItalic.ttf"
      },
      {
        "name": "Ubuntu-Medium",
        "file": "./assets/fonts/Ubuntu-Medium.ttf"
      },
      {
        "name": "Ubuntu-MediumItalic",
        "file": "./assets/fonts/Ubuntu-MediumItalic.ttf"
      },
      {
        "name": "Ubuntu-Regular",
        "file": "./assets/fonts/Ubuntu-Regular.ttf"
      }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}